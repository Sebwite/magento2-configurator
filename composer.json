{
  "name": "ctidigital/magento2-configurator",
  "description": "Keep magento persistently configured using files",
  "type": "magento2-module",
  "repositories": [
    {
      "0": {
        "type": "composer",
        "url": "https://repo.magento.com/"
      },
      "firegento_extendedimport2": {
        "type": "vcs",
        "url": "https://github.com/firegento/FireGento_ExtendedImport2"
      }
    }
  ],
  "require": {
    "php": "~5.6.0|~7.0.0",
    "symfony/yaml": ">2.8.0",
    "magento/framework": ">=100.1.0",
    "firegento/fastsimpleimport": "1.1.0",
    "firegento/extendedimport": "dev-master"
  },
  "require-dev": {
    "phpmd/phpmd": "@stable",
    "squizlabs/php_codesniffer": "1.5.3",
    "sebastian/phpcpd": "2.0.0",
    "satooshi/php-coveralls": "^1.0",
    "phpunit/phpunit": "4.1.0"
  },
  "version": "0.13.0-dev",
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "CtiDigital\\Configurator\\": ""
    }
  }
}
